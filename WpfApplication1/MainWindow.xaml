<Window x:Class="Stolowka.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Logowanko" Height="300" Width="300">
    <Grid>
        <Label Content="login" FontSize ="20" HorizontalAlignment="Left" Margin="120,4,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="login" HorizontalAlignment="Left" Height="23" Margin="85,41,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" RenderTransformOrigin="-0.043,0.205"/>
        <Label Content="hasło" FontSize ="20" HorizontalAlignment="Left" Margin="120,78,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.169,-1.284"/>
        <PasswordBox x:Name="pass" HorizontalAlignment="Left" Margin="85,115,0,0" VerticalAlignment="Top" Height="23" Width="120" RenderTransformOrigin="-0.06,-2.409"/>
        <Button Content="zaloguj" HorizontalAlignment="Left" Margin="108,165,0,0" VerticalAlignment="Top" Width="75" Click="zaloguj"/>
        <Button Content="zamknij" HorizontalAlignment="Left" Margin="108,209,0,0" VerticalAlignment="Top" Width="75" Click="zamknij"/>
    </Grid>
</Window>
